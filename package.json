  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/farhd/realworld-frontend-starter-kit.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/farhd/realworld-frontend-starter-kit/issues"
  },
  "homepage": "https://github.com/farhd/realworld-frontend-starter-kit#readme",
  "dependencies": {
    "mithril": "^2.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-transform-react-jsx": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "babel-loader": "^8.0.6",
    "bundlesize": "^0.18.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "husky": "^4.2.3",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "webpack": "^4.42.0",
